<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="pygubu.builder.widgets.scrolledframe" id="mainFrame">
    <property name="height">300</property>
    <property name="width">300</property>
    <property name="usemousewheel">true</property>
    <property name="scrolltype">vertical</property>
    <layout manager="pack">
      <property name="expand">true</property>
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="frame2">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Canvas" id="canvas">
            <property name="background">#9342ff</property>
            <property name="height">400</property>
            <property name="width">400</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="labelRulerFootHeight">
            <property name="anchor">center</property>
            <property name="font">TkDefaultFont</property>
            <property name="justify">center</property>
            <property name="text" translatable="yes">Độ cao chân thước:</property>
            <property name="width">20</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="txtRulerFootHeight">
            <property name="font">TkDefaultFont</property>
            <property name="justify">left</property>
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">0</property>
            <property name="width">18</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="labelRulerHeight">
            <property name="anchor">center</property>
            <property name="cursor">arrow</property>
            <property name="font">TkDefaultFont</property>
            <property name="text" translatable="yes">Độ cao thước:</property>
            <property name="width">20</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="txtRulerHeight">
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">100</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="labelThreeLinesHeight">
            <property name="anchor">center</property>
            <property name="font">TkDefaultFont</property>
            <property name="text" translatable="yes">Chiều cao mức thước:</property>
            <property name="width">20</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="txtThreeLinesHeight">
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">5</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="btnOpenFile">
            <property name="text" translatable="yes">Chọn file</property>
            <property name="width">13</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="btnDetectWaterLevel">
            <property name="text" translatable="yes">Phát hiện mực nước</property>
            <property name="width">20</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="btnQuit">
            <property name="text" translatable="yes">Thoát</property>
            <property name="width">13</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lblWaterLevel">
            <property name="font">{Arial CYR} 16 {bold}</property>
            <property name="foreground">#ff4242</property>
            <property name="justify">left</property>
            <property name="text" translatable="yes">0</property>
            <layout manager="pack">
              <property name="anchor">e</property>
              <property name="propagate">True</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
